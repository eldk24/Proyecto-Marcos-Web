      // Obtén los elementos del logo y el overlay
      const logo = document.getElementById('netflix-logo');
      const overlay = document.getElementById('overlay'); 

      // Calcula la duración total de la animación (en este caso 4 segundos: 1s + 1s + 1s + 1s)
      const animationDuration = 4;

      // Oculta el logo y el fondo después de que termine la animación con un desvanecimiento suave
